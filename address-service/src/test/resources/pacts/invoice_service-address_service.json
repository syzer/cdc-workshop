{
    "provider": {
        "name": "address_service"
    },
    "consumer": {
        "name": "invoice_service"
    },
    "interactions": [
        {
            "description": "get an address",
            "request": {
                "method": "GET",
                "path": "/addresses/1",
                "body": ""
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json; charset=UTF-8"
                },
                "body": {
                    "firstName": "Jan",
                    "street": "Limmat",
                    "surname": "Wloka"
                },
                "matchingRules": {
                    "$.body.firstName": {
                        "match": "type"
                    },
                    "$.body.surname": {
                        "match": "type"
                    },
                    "$.body.street": {
                        "match": "type"
                    }
                }
            }
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.3.6"
        }
    }
}